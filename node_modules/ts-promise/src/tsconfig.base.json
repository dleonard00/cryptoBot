{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"declaration": true,
		"noImplicitAny": true,
		"removeComments": false,
		"sourceMap": true,
		"noLib": false,
		"preserveConstEnums": true,
		"suppressImplicitAnyIndexErrors": true,
		"moduleResolution": "node",
		"outDir": "../dist/",
		"rootDir": ".",
		"types": [
			"node"
		],
		"lib": [
			"es5"
		]
	}
}